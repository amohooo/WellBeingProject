<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="paneDeveloper" prefHeight="781.0" prefWidth="600.0" style="-fx-background-color: #009ee0;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cab302.wellbeing.controller.DeveloperController">
   <children>
      <Label fx:id="lblDev" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <TableView fx:id="table" layoutX="63.0" layoutY="442.0" prefHeight="230.0" prefWidth="474.0">
        <columns>
          <TableColumn fx:id="colEmail" prefWidth="183.0" text="Email" />
          <TableColumn fx:id="colFeedback" prefWidth="272.0" text="Feedback" />
        </columns>
      </TableView>
      <TextField fx:id="txtMsg" layoutX="67.0" layoutY="274.0" prefHeight="87.0" prefWidth="462.0" />
      <Button fx:id="btnSend" layoutX="471.0" layoutY="377.0" mnemonicParsing="false" onAction="#sendMessage" style="-fx-background-color: transparent;" text="Send" textFill="WHITE">
         <font>
            <Font name="Segoe Print Bold" size="14.0" />
         </font>
         <effect>
            <DropShadow>
               <input>
                  <Glow />
               </input>
               <color>
                  <Color opacity="0.5687022805213928" />
               </color>
            </DropShadow>
         </effect>
      </Button>
      <Button fx:id="btnClose" layoutX="501.0" layoutY="732.0" mnemonicParsing="false" onAction="#closeDeveloper" style="-fx-background-color: transparent;" text="Close" textFill="WHITE">
         <font>
            <Font name="Segoe Print Bold" size="14.0" />
         </font>
         <effect>
            <DropShadow>
               <color>
                  <Color opacity="0.572519063949585" />
               </color>
               <input>
                  <Glow />
               </input>
            </DropShadow>
         </effect>
      </Button>
      <TextArea fx:id="txtDisplay" editable="false" layoutX="63.0" layoutY="49.0" prefHeight="200.0" prefWidth="474.0" />
      <Button fx:id="btnConnect" layoutX="67.0" layoutY="377.0" mnemonicParsing="false" onAction="#connectToServer" style="-fx-background-color: transparent;" text="Connect" textFill="WHITE">
         <font>
            <Font name="Segoe Print Bold" size="14.0" />
         </font>
         <effect>
            <DropShadow>
               <input>
                  <Glow />
               </input>
               <color>
                  <Color opacity="0.5687022805213928" />
               </color>
            </DropShadow>
         </effect>
      </Button>
   </children>
</AnchorPane>
